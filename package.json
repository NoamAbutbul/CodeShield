{
    "name": "code-shield",
    "displayName": "Code Shield",
    "description": "A VSCode extension that check security issues in your code",
    "version": "0.0.3",
    "publisher": "NoamAbutbulEliyahuAnter",
    "engines": {
        "vscode": "^1.91.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onCommand:codeShield.securityCheck"
    ],
    "contributes": {
        "commands": [
            {
                "command": "codeShield.securityCheck",
                "title": "Code Shield"
            }
        ]
    },
    "repository": {
    "type": "git",
    "url": "https://github.com/NoamAbutbul/CodeShield"
    },

    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.18.63",
        "@types/vscode": "^1.91.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5",
        "vscode": "^1.1.34",
        "vscode-test": "^1.6.1"
    },
    "directories": {
        "test": "tests"
    },
    "dependencies": {
        "agent-base": "^6.0.2",
        "balanced-match": "^1.0.2",
        "big-integer": "^1.6.52",
        "binary": "^0.3.0",
        "bluebird": "^3.4.7",
        "brace-expansion": "^1.1.11",
        "buffer-indexof-polyfill": "^1.0.2",
        "buffers": "^0.1.1",
        "chainsaw": "^0.1.0",
        "concat-map": "^0.0.1",
        "core-util-is": "^1.0.3",
        "debug": "^4.3.5",
        "duplexer2": "^0.1.4",
        "fs.realpath": "^1.0.0",
        "fstream": "^1.0.12",
        "glob": "^7.2.3",
        "graceful-fs": "^4.2.11",
        "http-proxy-agent": "^4.0.1",
        "https-proxy-agent": "^5.0.1",
        "inflight": "^1.0.6",
        "inherits": "^2.0.4",
        "isarray": "^1.0.0",
        "listenercount": "^1.0.1",
        "marked": "^13.0.3",
        "minimatch": "^3.1.2",
        "minimist": "^1.2.8",
        "mkdirp": "^0.5.6",
        "ms": "^2.1.2",
        "once": "^1.4.0",
        "path-is-absolute": "^1.0.1",
        "process-nextick-args": "^2.0.1",
        "readable-stream": "^2.3.8",
        "rimraf": "^3.0.2",
        "safe-buffer": "^5.1.2",
        "setimmediate": "^1.0.5",
        "string_decoder": "^1.1.1",
        "traverse": "^0.3.9",
        "unzipper": "^0.10.14",
        "util-deprecate": "^1.0.2",
        "wrappy": "^1.0.2"
    },
    "keywords": [],
    "author": "Noam Abutbul & Eliyahu Anter",
    "license": "MIT",
    "icon": "assets/logo.png" 
}
